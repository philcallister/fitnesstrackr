<h1>Editing Single Plan</h1>
<% if @workout_plan -%>
  <h2><%= h @workout_plan.workout_block.program.name %>&nbsp;>&nbsp;<%= h @workout_plan.workout_block.name %>&nbsp;>&nbsp;<%= h @workout_plan.name %></h2>
<% end -%>
<% form_for(@single_plan) do |f| %>
  <%= f.error_messages %>
  Notes<br />
  <%= f.text_field :notes %><br />
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>
<br />
<% if @workout_plan %>
  <%= link_to 'Back', workout_plan_exercise_plans_path(@workout_plan) %>
<% end -%>