<% content_for :content do -%>
  <div class="container_12 clearfix content">
    <div class="grid_12">
      <h1>Listing Programs</h1>
      <table>
        <tr>
          <th>Name</th>
          <th>Description</th>
          <th>Actions</th>
        </tr>
        <% @programs.each do |program| %>
          <tr>
            <td><%= h program.name %></td>
            <td><%= h program.description %></td>
            <td>
              <%= link_to 'Show', program %>&nbsp;&nbsp;|&nbsp;
              <%= link_to 'Edit', edit_program_path(program) %>&nbsp;&nbsp;|&nbsp;
              <%= link_to 'Destroy', program, :confirm => 'Are you sure?', :method => :delete %>&nbsp;&nbsp;|&nbsp;
              <%= link_to 'Workout Blocks', program_workout_blocks_path(program) %>
            </td>
          </tr>
        <% end %>
      </table>
      <br />
      <%= link_to 'New Program', new_program_path %>
    </div>
  </div>
<% end %>