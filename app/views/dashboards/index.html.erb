<% content_for :content do -%>
  <div id="dashboard" class="container_12 clearfix">
    <div id="goal" class="container_12 clearfix">
      <div id="" class="grid_12 dashboard-section" style="margin-top: 10px;">
        <div style="font-size: 1.25em; margin: 15px 10px 15px 10px;">
          <span class="dashboard-header">Goal</span><span class="dashboard-header-copy">Complete the Waupaca Triathlon in under 2 hours&nbsp;&nbsp;/&nbsp;&nbsp;Swim under 17 minutes</span>
        </div>
        <hr />
        <div style="padding: 5px 10px 15px 10px; font-size: 1.25em;">
          <div style="display: inline-block;">
            <span class="dashboard-item-left">Start Date</span><span class="dashboard-item-right">March 3rd, 2010</span>
          </div>
          <div style="display: inline-block; margin-left: 15px;">
            <span class="dashboard-item-left">End Date</span><span class="dashboard-item-right">August 19th, 2010</span>
          </div>
        </div>
      </div>
    </div>
    <div id="workout" class="container_12 clearfix">
      <div id="" class="grid_6 dashboard-section" style="margin: 10px 9px 0px 9px;">
        <div style="font-size: 1.25em; margin: 15px 10px 15px 10px;">
          <span class="dashboard-header">Total Workouts</span><span class="dashboard-header-copy">Your total workout progress</span>
        </div>
        <hr />
        <div class="meter-wrap" style="padding: 1px; margin: 5px 10px 10px 10px;">
          <div class="meter-value" style="width: 47%;">
            <div class="meter-text">
              33 of 70 Workouts Completed (47%)
            </div>
          </div>
        </div>
      </div>
      <div id="" class="grid_6 dashboard-section" style="margin: 10px 9px 0px 9px;">
        <div style="font-size: 1.25em; margin: 15px 10px 15px 10px;">
          <span class="dashboard-header">Trainer Workouts</span><span class="dashboard-header-copy">Personal Trainer workout progress</span>
        </div>
        <hr />
        <div class="meter-wrap" style="padding: 1px; margin: 5px 10px 10px 10px;">
          <div class="meter-value" style="width: 35%;">
            <div class="meter-text">
              7 of 20 Workouts Completed (35%)
            </div>
          </div>
        </div>
      </div>
    </div>
    <div id="current" class="container_12 clearfix">
      <div id="" class="grid_4 dashboard-section" style="height: 275px; margin: 10px 9px;">
        <div style="margin-top: 5px; padding: 10px; font-size: 1.25em;">
          <span class="dashboard-marker">Measurements</span>
        </div>
        <hr />
        <div>
          <ul class="item">
            <li><div class="dashboard-item-left">Age</div><div class="dashboard-item-right">41</div></li>
            <li><div class="dashboard-item-left">Height</div><div class="dashboard-item-right">5'10"</div></li>
            <li><div class="dashboard-item-left">Weight</div><div class="dashboard-item-right">182</div></li>
            <li><div class="dashboard-item-left">BMI</div><div class="dashboard-item-right">26.07</div></li>
            <li><div class="dashboard-item-left">V02 Max</div><div class="dashboard-item-right">None</div></li>
          </ul>
        </div>
      </div>
      <div id="" class="grid_4 dashboard-section" style="height: 275px; margin: 10px 9px;">
        <div class="" style="margin-top: 5px; padding: 10px; font-size: 1.25em;">
          <span class="dashboard-marker">Program</span>
        </div>
        <hr />
      </div>
      <div id="" class="grid_4 dashboard-section" style="height: 275px; margin: 10px 9px;">
        <div class="" style="margin-top: 5px; padding: 10px; font-size: 1.25em;">
          <span class="dashboard-marker">Workout Plan</span>
        </div>
        <hr />
      </div>
    </div>
  </div>
<% end -%>