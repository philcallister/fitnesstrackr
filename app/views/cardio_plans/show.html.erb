<h1>Showing Cardio Plan</h1>
<% if @workout_plan %>
  <h2><%= h @workout_plan.program.name %></h2>
  <h2><%= h @workout_plan.name %></h2>
<% end -%>
<table cellpadding="0" cellspacing="0">
  <tr>
    <td class="item">Notes</td>
    <td><%= h @cardio_plan.notes %>
  </tr>
  <tr>
    <td class="item">Time</td>
    <td><%= h @cardio_plan.time %>
  </tr>
  <tr>
    <td class="item">Level</td>
    <td><%= h @cardio_plan.level %>
  </tr>
  <tr>
    <td class="item">Incline</td>
    <td><%= h @cardio_plan.incline %>
  </tr>
  <tr>
    <td class="item">Speed</td>
    <td><%= h @cardio_plan.speed %>
  </tr>
  <tr>
    <td class="item">Distance</td>
    <td><%= h @cardio_plan.distance %>
  </tr>
  <tr>
    <td class="item">Calories</td>
    <td><%= h @cardio_plan.calories %>
  </tr>
  <tr>
    <td class="item">Min Heartrate</td>
    <td><%= h @cardio_plan.min_hr %>
  </tr>
  <tr>
    <td class="item">Max Heartrate</td>
    <td><%= h @cardio_plan.max_hr %>
  </tr>
</table>
<br />
<% if @workout_plan %>
  <%= link_to 'Edit', edit_workout_plan_cardio_plan_path(@workout_plan, @cardio_plan) %> |
  <%= link_to 'Back', workout_plan_exercise_plans_path(@workout_plan) %>
<% end -%>