<% content_for :content do -%>
  <div class="container_12 clearfix content">
    <div class="grid_12" style="padding-bottom: 15px;">
      <h1>Register</h1>

      <% form_for @user, :url => account_path do |f| %>
        <%= f.error_messages %>
        <%= render :partial => "form", :object => f %><br />
        <%= f.submit "Register" %>
      <% end %>
    </div>
  </div>
<% end %>